.container{
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background-color: #fff;
}
.add-btn{
  width: 80rpx;
  height: 80rpx;
  border: 6rpx solid #5e5f5fc5;
  border-radius: 50%;
  position: relative;
}
.add-btn::before{
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 6rpx;
  height: 50%;
  background-color: #5e5f5fc5;
}
.add-btn::after{
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 50%;
  height: 6rpx;
  background-color: #5e5f5fc5;
}